# Contact Information
# These environment variables override the default contact configuration in src/config/contact.ts
# Copy this file to .env.local and update with your actual values

# Phone Number Configuration
# Country code (e.g., +995 for Georgia)
VITE_PHONE_COUNTRY_CODE=+995

# Phone number without country code (9 digits for Georgian mobile)
# Display format: 558 211 584
VITE_PHONE_NUMBER=558211584

# WhatsApp Number
# International format without + or spaces (e.g., 995558211584)
# Note: Custom wa.link URL is used (https://wa.link/1a2bes)
VITE_WHATSAPP_NUMBER=995558211584

# Email Address
VITE_CONTACT_EMAIL=info@rentals.ge

# Physical Address
VITE_CONTACT_ADDRESS=Tbilisi, Gia Abesadze 10

# Google Maps Embed URL
# Get your embed URL from: https://www.google.com/maps
# Search for your address, click "Share" > "Embed a map" and copy the src URL
VITE_MAP_EMBED_URL=https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2977.171394710362!2d44.7832!3d41.7151!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zNDHCsDQyJzU0LjQiTiA0NMKwNDYnNTkuNSJF!5e0!3m2!1sen!2sge!4v1234567890

# Social Media Links (optional)
VITE_FACEBOOK_URL=
VITE_INSTAGRAM_URL=
VITE_TWITTER_URL=

# Supabase Configuration (from your Supabase project)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key_here

# Resend Email Configuration (for sending transactional emails)
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxx

# Email "From" address (must be verified in Resend)
# Format: "Your Company <noreply@yourdomain.com>"
RESEND_FROM=Rentals Georgia <noreply@rentalsgeorgia.com>

# Admin notification email (receives booking notifications)
BOOKING_NOTIFICATION_EMAIL=nikashio42@gmail.com

# Admin notification email (receives contact form submissions)
CONTACT_TO_EMAIL=pegageorgia@gmail.com

# Note: After updating .env.local, restart the dev server for changes to take effect
